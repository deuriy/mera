.SocLink
	display inline-flex
	width 3.5em
	height 3.5em
	border 1px solid transparent
	border-radius 50%
	background $white
	font-size 16px
	filter drop-shadow(0px 12px 40px rgba(0, 0, 0, 0.15))
	@extend .transition

	&:hover
		border-color $blue2
		background-color $blue2
	
	&-outline
		border-color rgba($grey, 0.5)
		filter none

